<%|
    Array[String[1]] $environment,
    Array[Sensitive[String[1]]] $sensitive_environment,
    String $cron_cmd
|%>
#!/bin/sh
<%- $environment.each |environment| { -%>
export <%= environment %>
<%- } -%>
<%- $sensitive_environment.each |environment| { -%>
export <%= environment %>
<%- } -%>
<%= $cron_cmd %>
